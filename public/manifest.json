{
	"name": "Quickmeme",
	"version": "3.0",
	"description": "A quick and subtle way to look at memes!",
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "quickmeme_16.png",
			"32": "quickmeme_32.png",
			"48": "quickmeme_48.png",
			"128": "quickmeme_128.png"
		}
	},
	"icons": {
		"16": "quickmeme_16.png",
		"32": "quickmeme_32.png",
		"48": "quickmeme_48.png",
		"128": "quickmeme_128.png"
	},
	"manifest_version": 2
}